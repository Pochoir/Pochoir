		((pa . interior(t-1, z+0, y+0, x+0). _S)) = (1.0/18.0)*rho*(1.0 +       uy*(4.5*uy       - 3.0) - u2);
		((pa . interior(t-1, z+0, y+0, x+0). _E)) = (1.0/18.0)*rho*(1.0 +       ux*(4.5*ux       + 3.0) - u2);
		((pa . interior(t-1, z+0, y+0, x+0). _W)) = (1.0/18.0)*rho*(1.0 +       ux*(4.5*ux       - 3.0) - u2);
