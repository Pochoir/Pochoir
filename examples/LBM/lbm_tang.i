  {
    volume *= m_algo.phys_length_ [i] ;
    if (grid.x1 [i] - grid.x0 [i] > W)
    {
      W = grid.x1 [i] - grid.x0 [i] ;
      slope = m_algo.slope_ [i] ;
    }
  }
  time_type dag_time = 0 ;
  time_type expected_run_time = 0 ;
