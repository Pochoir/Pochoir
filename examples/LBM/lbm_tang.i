    cout << "Predicted " ;
    stopwatch_print_elapsed_time(expected_run_time) ;
    */
  }
  else
  {
    m_initial_height = h1 ;
    initialize(grid, h1, h2, false, depth) ;
    //back up data
    //copy_data(m_array, array, volume * depth) ;
    //*array_addr = m_array ; //change data in pochoir array
    //do a dry run
    time_type t ;
    { clock_gettime(1, &(stopwatch_ptr->start)); } ;
