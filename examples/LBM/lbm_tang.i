                  int offset = t0 + T / h1 * h1 ;
                  expected_run_time += m_zoids[m_head[0]].time * (int) (T / h1) ;
                  h2 = t1 - offset ;
                  index = 1 ;
                  while (h2 >= 1)
                  {
                          //find index of most significant bit that is set
                          index_msb = (sizeof(int) << 3) - __builtin_clz(h2) - 1 ;
                          int h = 1 << index_msb ;
                          //cout << "t0 " << t0 << " t1 " << t1 << 
