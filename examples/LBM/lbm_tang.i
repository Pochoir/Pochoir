	}

	rho = + (((pa . interior(t-1, z+0, y+0, x+0). _C))) + (((pa . interior(t-1, z+0, y+0, x+0). _N)))
	      + (((pa . interior(t-1, z+0, y+0, x+0). _S))) + (((pa . interior(t-1, z+0, y+0, x+0). _E)))
	      + (((pa . interior(t-1, z+0, y+0, x+0). _W))) + (((pa . interior(t-1, z+0, y+0, x+0). _T)))
