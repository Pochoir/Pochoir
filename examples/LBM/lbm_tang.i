      dt_recursive_ = 1;
      dt_recursive_boundary_ = 1 ;
      dx_recursive_[0] = 1;
      dx_recursive_boundary_[0] = 1 ;
      for (int i = N_RANK-1; i >= 1; --i) {
        dx_recursive_[i] = 1;
        dx_recursive_boundary_[i] = 1 ;
      }
    }

