namespace std
{
  /// initializer_list
  template<class _E>
    class initializer_list
    {
      const _E* __array;
      size_t __len;

      // The compiler can call a private constructor.
      initializer_list(const _E* __a, size_t __l)
      : __array(__a), __len(__l) { }

    public:
      initializer_list()
      : __array(__null), __len(0) { }

      // Number of elements.
      size_t size() const
      { return __len; }

      // First element.
      const _E* begin() const
      { return __array; }

      // One past the last element.
      const _E* end() const
      { return begin() + size(); }
  };
}

