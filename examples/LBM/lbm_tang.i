	{
		(static_cast<void> (0)) ;
		(static_cast<void> (0)) ;
		return phys_length_ [i] ;
	}

	inline int get_slope(int i)
	{
		(static_cast<void> (0)) ;
		(static_cast<void> (0)) ;
		return slope_ [i] ;
	}
    
    /* constructor */
    Algorithm (int const _slope[]) : dt_recursive_boundary_(1), r_t(1) {
//#if 0
        for (int i = 0; i < N_RANK; ++i) {
            slope_[i] = _slope[i];
            dx_recursive_boundary_[i] = _slope[i];
