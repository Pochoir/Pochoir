		rho = 1.0;

		ux = 2*ux1 - ux2;
		uy = 2*uy1 - uy2;
		uz = 2*uz1 - uz2;

		u2 = 1.5 * (ux*ux + uy*uy + uz*uz);

		((pa . interior(t-1, z+0, y+0, x+0). _C)) = (1.0/ 3.0)*rho*(1.0                                 - u2);

